      "source": [
        "n = int(input(\"Введите число от 1 до 10: \"))\n",
        "s = [1]\n",
        "\n",
        "for i in range(n):\n",
        "    a = \" \".join(str(x) for x in s)\n",
        "    print(a.center(n * 4))\n",
        "\n",
        "    s1 = [1]\n",
        "    for j in range(len(s) - 1):\n",
        "        s1.append(s[j] + s[j + 1])\n",
        "    s1.append(1)\n",
        "\n",
        "    s = s1"
